{
  "id": null,
  "title": "INKSPOT Database Performance",
  "tags": ["inkspot", "business", "analytics"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total Users",
      "type": "stat",
      "targets": [
        {
          "expr": "pg_database_size_bytes{datname=\"inkspot\"} / 1024 / 1024",
          "refId": "A",
          "legendFormat": "Database Size (MB)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "basic"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
