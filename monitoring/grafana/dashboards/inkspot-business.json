{
  "dashboard": {
    "id": null,
    "title": "INKSPOT - Business Metrics",
    "tags": ["inkspot", "business", "revenue"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Daily Revenue",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(inkspot_revenue_total[1d])",
            "refId": "A",
            "legendFormat": "Daily Revenue"
          }
        ],
        "yAxes": [
          {
            "unit": "currencyEUR",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Booking Conversion Funnel",
        "type": "graph",
        "targets": [
          {
            "expr": "inkspot_bookings_pending",
            "refId": "A",
            "legendFormat": "Pending Bookings"
          },
          {
            "expr": "inkspot_bookings_confirmed",
            "refId": "B",
            "legendFormat": "Confirmed Bookings"
          },
          {
            "expr": "inkspot_bookings_completed",
            "refId": "C",
            "legendFormat": "Completed Bookings"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "PRO User Statistics",
        "type": "stat",
        "targets": [
          {
            "expr": "inkspot_users_pro",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Payment Status Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "inkspot_payments_successful",
            "refId": "A",
            "legendFormat": "Successful"
          },
          {
            "expr": "inkspot_payments_pending",
            "refId": "B",
            "legendFormat": "Pending"
          },
          {
            "expr": "inkspot_payments_failed",
            "refId": "C",
            "legendFormat": "Failed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "User Engagement Score",
        "type": "stat",
        "targets": [
          {
            "expr": "(inkspot_likes_today + inkspot_messages_today + inkspot_posts_today) / inkspot_users_active_today",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 3
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Average Rating",
        "type": "gauge",
        "targets": [
          {
            "expr": "inkspot_rating_average",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 1,
            "max": 5,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 1
                },
                {
                  "color": "yellow",
                  "value": 3
                },
                {
                  "color": "green",
                  "value": 4
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Top Search Terms",
        "type": "table",
        "targets": [
          {
            "target": "search_trends",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5m",
    "schemaVersion": 30,
    "version": 0,
    "links": []
  }
}